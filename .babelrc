{
  "presets": [
    ["@babel/preset-env", {
      // "modules": false, //commented to enanle es6 modules for jest
      "targets": {
        "browsers": "> 0.25%, ie 9, not op_mini all, not dead",
        "node": 'current'
      }
    }],
  ],

  "plugins": [
    ["@babel/plugin-proposal-decorators", {legacy: true}],
    "@babel/plugin-proposal-export-default-from",
    ["@babel/plugin-proposal-class-properties", {loose: false}]
  ]
}
